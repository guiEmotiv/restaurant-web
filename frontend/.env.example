# AWS Cognito Configuration
# Copy this file to .env.local and fill in your AWS Cognito details

# AWS Region where your Cognito User Pool is located
REACT_APP_AWS_REGION=us-east-1

# Cognito User Pool ID (found in AWS Cognito console)
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX

# Cognito App Client ID (found in AWS Cognito console)  
REACT_APP_COGNITO_APP_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Cognito Domain (optional, for hosted UI)
REACT_APP_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com

# Redirect URLs
REACT_APP_REDIRECT_SIGN_IN=http://localhost:5173
REACT_APP_REDIRECT_SIGN_OUT=http://localhost:5173