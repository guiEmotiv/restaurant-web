# Frontend Production Environment
# Copy to frontend/.env.production for production builds

# AWS Cognito Configuration
VITE_AWS_REGION=us-east-1
VITE_COGNITO_USER_POOL_ID=us-east-1_YOUR_POOL_ID
VITE_COGNITO_APP_CLIENT_ID=YOUR_APP_CLIENT_ID
VITE_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com

# API Configuration
VITE_API_BASE_URL=https://www.xn--elfogndedonsoto-zrb.com
VITE_API_VERSION=v1

# Authentication Redirects
VITE_REDIRECT_SIGN_IN=https://www.xn--elfogndedonsoto-zrb.com
VITE_REDIRECT_SIGN_OUT=https://www.xn--elfogndedonsoto-zrb.com

# Production Settings
VITE_DEBUG_MODE=false
VITE_ENABLE_DEVTOOLS=false
VITE_LOG_LEVEL=error

# Application Settings
VITE_APP_NAME=El Fog√≥n de Don Soto
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_BLUETOOTH_PRINTER=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance
VITE_ENABLE_SOURCE_MAPS=false
VITE_CHUNK_SIZE_WARNING_LIMIT=500

# Monitoring
VITE_SENTRY_DSN=https://your-frontend-sentry-dsn@sentry.io/project-id
VITE_ANALYTICS_ID=GA_MEASUREMENT_ID