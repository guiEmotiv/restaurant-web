# Frontend Development Environment
# Copy to frontend/.env.local for development

# AWS Cognito Configuration
VITE_AWS_REGION=us-east-1
VITE_COGNITO_USER_POOL_ID=us-east-1_YOUR_POOL_ID
VITE_COGNITO_APP_CLIENT_ID=YOUR_APP_CLIENT_ID
VITE_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=v1

# Authentication Redirects
VITE_REDIRECT_SIGN_IN=http://localhost:5173
VITE_REDIRECT_SIGN_OUT=http://localhost:5173

# Development Features
VITE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# Application Settings
VITE_APP_NAME=Restaurant Management System
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_BLUETOOTH_PRINTER=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false