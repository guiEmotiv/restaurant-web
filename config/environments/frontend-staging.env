# Frontend Staging Environment
# Copy to frontend/.env.staging for staging builds

# AWS Cognito Configuration
VITE_AWS_REGION=us-east-1
VITE_COGNITO_USER_POOL_ID=us-east-1_YOUR_POOL_ID
VITE_COGNITO_APP_CLIENT_ID=YOUR_APP_CLIENT_ID
VITE_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com

# API Configuration
VITE_API_BASE_URL=https://staging.restaurant-app.com
VITE_API_VERSION=v1

# Authentication Redirects
VITE_REDIRECT_SIGN_IN=https://staging.restaurant-app.com
VITE_REDIRECT_SIGN_OUT=https://staging.restaurant-app.com

# Production-like Settings
VITE_DEBUG_MODE=false
VITE_ENABLE_DEVTOOLS=false
VITE_LOG_LEVEL=info

# Application Settings
VITE_APP_NAME=Restaurant Management System (Staging)
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_BLUETOOTH_PRINTER=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true

# Performance
VITE_ENABLE_SOURCE_MAPS=false
VITE_CHUNK_SIZE_WARNING_LIMIT=1000